{
  "name": "notetify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "cy:open": "cypress open",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:docker": "cross-env HOST=0.0.0.0 NODE_ENV=development concurrently \"npm:docker:ws\"  \"vite --host 0.0.0.0 --port 5173\"",
    "docker:ws": "cross-env HOST=0.0.0.0 PORT=1234 YPERSISTENCE=./yjs-wss-db npx y-websocket",
    "server:ws": "cross-env HOST=localhost PORT=1234 YPERSISTENCE=./yjs-wss-db npx y-websocket",
    "dev:local": "cross-env NODE_ENV=development concurrently \"npm:server:ws\" \"vite\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@lexical/react": "^0.23.1",
    "@lexical/yjs": "^0.24.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-query": "^5.71.10",
    "axios": "^1.7.7",
    "js-cookie": "^3.0.5",
    "lexical": "^0.23.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-router": "^7.5.3",
    "tailwindcss": "^4.1.5",
    "universal-cookie": "^7.2.2",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^2.1.0",
    "yjs": "^13.6.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.1.2"
  }
}
